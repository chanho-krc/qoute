{
  "expo": {
    "name": "오늘의 명언",
    "slug": "daily-wisdom-quotes",
    "version": "1.2.0",
    "orientation": "portrait",
    "icon": "./assets/images/quote-app-icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "platforms": [
      "android",
      "ios",
      "web"
    ],
    "newArchEnabled": true,
    "primaryColor": "#90caf9",
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.dailywisdom.quotes",
      "buildNumber": "2",
      "icon": "./assets/images/quote-app-icon.png"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/quote-app-adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.dailywisdom.quotes",
      "versionCode": 5,
      "permissions": [
        "INTERNET",
        "RECEIVE_BOOT_COMPLETED",
        "WAKE_LOCK"
      ],
      "icon": "./assets/images/quote-app-icon.png"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/quote-app-favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "backgroundColor": "#1e1e1e"
        }
      ],
      [
        "expo-notifications",
        {
          "icon": "./assets/images/quote-app-icon.png",
          "color": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "b555daef-93cb-4258-97a6-f61c04b9fc8d"
      }
    },
    "owner": "rohbin00",
    "runtimeVersion": {
      "policy": "appVersion"
    }
  }
}
